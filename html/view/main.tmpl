<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <link rel="stylesheet" type="text/css" href="{{ css }}">
    <title>{{ title }}</title>
</head>
<body>
<div class="wrapper">

    {% include 'header.tmpl' %}
    <div class="content">
        {% if right %}
        {% include 'rightCol.tmpl' %}
        {% endif %}
        <div class="main">

            {% if index %}
            {{ index|raw }}
                {% include 'order.tmpl' %}
            {% endif %}

            {% if tel %}
                <div class="none">
                {{ tel|raw }}
                </div>
            {% endif %}

            {% if pechkin %}
                <div class="none">
                    {{ pechkin|raw }}
                </div>
            {% endif %}

            {% if content %}
            {% include content %}
            {% endif %}



        </div>
    </div>
    {% include 'footer.tmpl' %}

</div>
</body>
</html>